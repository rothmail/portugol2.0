programa {
  funcao inicio() {
   inteiro num
   faca{
   escreva("Digite um número:\n")
   leia(num)

      se (num > 5) {
         escreva("Maior que 5\n")
        } senao se (num < 5) {
         escreva("Menor que 5\n")
        } senao {
         escreva("Resultado não encontrado\n")
      }
   } enquanto (num != 5) 
  }
}
