programa {
  funcao inicio() {
    inteiro numero
	escreva("digite um número: ","\n")
	leia(numero)
	enquanto (numero >= 0 e numero <= 10) {
		escreva(numero * 0, "\n")
		escreva(numero * 1, "\n")
		escreva(numero * 2, "\n")
		escreva(numero * 3, "\n")
		escreva(numero * 4, "\n")
		escreva(numero * 5, "\n")
		escreva(numero * 6, "\n")
		escreva(numero * 7, "\n")
		escreva(numero * 8, "\n")
		escreva(numero * 9, "\n")
		escreva(numero * 10, "\n")
		pare
	}
  }
}
