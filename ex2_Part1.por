programa {
  funcao inicio() {
    inteiro num1 = 5
    inteiro num2 = 20

    logico resultado1 = (num1 > num2)
    logico resultado2 = (num1 < num2)
    logico resultado3 = (num1 >= num2)
    logico resultado4 = (num1 <= num2)
